{
  "compilerOptions": {
    "jsx": "react",
    "lib": ["es5", "es6"],
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,

  },
  "include": ["src/**/*", "ormconfig.js", "src/ormconfig.js"],
  "exclude": ["node_modules", "client"]
}
